public class ForLoopDemo {

	public static void main(String[] args) {
		/*for (int i = 0; i <= 10; i++)
		int i = 0;
		while(i <= 10) {
			System.out.printf(" %d  ", i);
			i++;
		}*/
		int  num = 4;
		for (int i = num-1; i > 0; i--)
		num = num * i;
		System.out.println("Factorial is : " + num);
	}
	
}
